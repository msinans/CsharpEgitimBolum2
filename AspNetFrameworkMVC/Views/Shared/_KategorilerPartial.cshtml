@{
    List<string> kategoriler = new List<string>()
    {
        "Bilgisayar", "Monitör", "Klavye", "Telefon", "Laptop"
 };
}

<ul class=" =List-group">
    @foreach (var kategori in kategoriler)
    {
        <li class="list-group-item">
            <a href="#">
                @kategori
            </a>                   
        </li> 
    }
    </ul>